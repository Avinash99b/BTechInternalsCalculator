{
  "cli": {
    "version": ">= 16.25.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EAS_UPDATE_BRANCH": "staging"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EAS_UPDATE_BRANCH": "production"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
